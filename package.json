{
  "name": "cypressqa_login",
  "version": "1.0.0",
  "description": "Caso de prueba del login home page toctoc",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --reporter mochawesome",
    "cy:run:login": "cypress run --browser chrome --record --key 5acce98b-ac39-4afa-97e6-721028ccc35d"
  },
  "keywords": [
    "QA",
    "Automation",
    "Login"
  ],
  "author": "Mariela Hurtado",
  "license": "ISC",
  "dependencies": {
    "cypress": "^7.1.0",
    "cypress-cucumber-preprocessor": "^4.1.0",
    "cypress-xpath": "^1.6.2",
    "mochawesome": "^6.2.2"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/features/step_definitions/**/",
    "cucumberJson": {
      "generate": false,
      "outputFolder": "cypress/cucumber-report",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mariela2020/CypreessQA_Login"
  },
  "devDependencies": {
    "cucumber-html-reporter": "^5.4.0"
  }
}
